{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base"
    ],
    "packageRules": [
        {
            "automerge": true,
            "matchUpdateTypes": [
                "minor",
                "patch",
                "pin",
                "digest"
            ],
            "minimumReleaseAge": "3 days"
        }
    ],
    "baseBranches": [
        "develop"
    ]
}
